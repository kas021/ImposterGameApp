{
  "cli": {
    "version": ">= 16.23.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_VERSION": "20.19.0",
        "EXPO_NO_CACHE": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NODE_VERSION": "20.19.0",
        "EXPO_NO_CACHE": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_VERSION": "20.19.0",
        "EXPO_NO_CACHE": "1"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
